<!DOCTYPE html>
<html>
<head>
<style>
table, th, td {
  border: 1px solid black;
}
</style>
</head>
<body>


<table style="width:20%">
    <tr>
        <th><h4>Module ID</h4></th>
        <th><h4>Name</h4></th>
        <th><h4>Credits</h4></th>
        <th><h4>Update</h4></th>
        <th><h4>Students</h4></th>
        
    </tr>
    <% courses.forEach((course) => { %>
        <tr>
            <td><%= course.mid %></td>
            <td><%= course.name %></td>
            <td><%= course.credits %></td>
            <td><a href="http://localhost:3000/editModule">Update</a></td>
            <td><a href="http://localhost:3000/module/student/:mid">List</a></td>
          

        </tr>
  <%  }) %>
</table>
<a href="http://localhost:3000/">Home</a>

</body>
</html>